{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ],
  "env": {
    "es6": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "warn",
    "no-unused-vars": "warn",
    "jsx-a11y/alt-text": "off",
    "@next/next/no-img-element": "off",
    "no-empty": "off",
    "import/no-anonymous-default-export": "off"
  }
}